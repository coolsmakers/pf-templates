{
  "name": "JsBot",
  "display": "JsBot",
  "type": "nodejs",
  "install": [
    {
      "commands": [
        "touch bot.js",
        "touch requirements.txt"
      ],
      "type": "command"
    },
    {
      "commands": [
        "chmod +x requirements.txt",
        "chmod +x bot.js"
      ],
      "type": "command"
    }
